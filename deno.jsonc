{
  "tasks": {
    "start": "deno run --allow-env --allow-net --allow-read mod.ts",
    "dev": "deno run --watch --allow-env --allow-net --allow-read mod.ts",
    "check": "deno check mod.ts",
    "cache": "deno cache mod.ts",
    "fmt": "deno run --allow-read --allow-write npm:prettier . --write",
    "lint": "deno lint",
    "test": "deno test",
    "compile": "deno compile --allow-env --allow-net --allow-read mod.ts"
  },
  "imports": {
    "@framework/": "./src/framework/",
    "@commands/": "./src/commands/",
    "@components/": "./src/components/",
    "@core": "./src/core.ts",
    "@events/": "./src/events/",
    "@typings/": "./src/typings/",
    "@utils/": "./src/utils/",
    "canvas/": "https://deno.land/x/canvas@v1.4.1/",
    "gtr/": "https://deno.land/x/gtr@v0.0.1/",
    "harmony/": "https://deno.land/x/harmony@v2.8.0/",
    "hono/": "https://deno.land/x/hono@v3.1.4/",
    "postgres/": "https://deno.land/x/postgres@v0.17.0/",
    "std/": "https://deno.land/std@0.181.0/"
  },
  "lock": false
}
